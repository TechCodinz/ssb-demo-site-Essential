{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/cloud",
            "destination": "/cloud.html"
        },
        {
            "source": "/cloud/login",
            "destination": "/cloud-login.html"
        },
        {
            "source": "/cloud/register",
            "destination": "/cloud-register.html"
        },
        {
            "source": "/cloud/dashboard",
            "destination": "/cloud-dashboard.html"
        },
        {
            "source": "/download",
            "destination": "/download.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}